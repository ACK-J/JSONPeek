{
  "manifest_version": 3,
  "name": "JSONPeek",
  "description": "Increments a counter for JSONP callback responses.",
  "version": "1.0",
"permissions": [
  "tabs",
  "webRequest",
  "webRequestBlocking",
  "storage",
  "activeTab"
],
  "background": {
    "scripts": ["background.js"]
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "<all_urls>"
  ]
}

