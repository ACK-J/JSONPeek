{
  "manifest_version": 3,
  "name": "JSONPeek",
  "description": "Listen for JSONP endpoints.",
  "version": "1.0",
"permissions": [
  "tabs",
  "webRequest",
  "webRequestBlocking",
  "storage",
  "activeTab"
],
  "background": {
    "scripts": ["background.js"]
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
  "512": "icons/eye-512.png",
  "128": "icons/eye-128.png"
}

}

